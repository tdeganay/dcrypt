<div id="mySidebar" class="sidebar" data-controller="sidebar" >
  <div class="sidebar-links">

    <%= link_to dashboard_path(current_user.dashboard), class: "sidebar-icon #{"active" if params[:action] == "show" }" do %>
        <i class="fas fa-cubes"></i> <p class="d-none"  data-sidebar-target="sidebar">Blocks</p>
      <% end %>
    <%= link_to "#", class: "sidebar-icon", remote: true do %>
        <i class="fas fa-chart-bar"></i> <p class="d-none"  data-sidebar-target="sidebar">Statistics</p>

      <% end %>
    <%= link_to pages_path, class: "sidebar-icon #{"active" if params[:action] == "notifications"}" do %>
        <i class="far fa-bell"></i> <p class="d-none"  data-sidebar-target="sidebar">Notifications</p>

      <% end %>
    <%= link_to "#", class: "sidebar-icon", remote: true do %>
     <i class="far fa-user" ></i> <p class="d-none" data-sidebar-target="sidebar">Profile</p>
    <% end %>
  </div>

  <div class="wrapper">
      <div class="sun-quote-pages">
  <i class="sidebar-icon goright far fa-arrow-alt-to-right" data-action="click->sidebar#close"></i>
      </div>
    </div>

</div>
